doctype html
head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    title Cloud Goverment Service
    link(rel="icon" type="image/png" href="../images/favicon.png")
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65' crossorigin='anonymous')
    link(rel='stylesheet' href='../stylesheets/style.css')
    link(rel='stylesheet' href='../stylesheets/normalize.css')
div.mova
    a#movaeng.mova-text.mova-active(href="#") English
    a#movaderz.mova-text(href="../uk-UA/profile") Державна
    a#movaork.mova-text(href="../ru-RU/profile") Русский
header#head
    .container
        .row
            .col-lg-12.col-sm-12.nav-bg
                nav.navbar.navbar-expand-lg
                    .container-fluid
                        a.navbar-brand(href='home')
                            img.navbar-logo(src='../images/textlogo.webp' alt='')
                        button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')
                            span.navbar-toggler-icon
                        #navbarNav.collapse.navbar-collapse
                            ul.navbar-nav
                                li.nav-item
                                    a.nav-link(href='home') Home
                                li.nav-item
                                    a.nav-link(href='home#prising') Prising Plan
                                li.nav-item
                                    a.nav-link(href='home#faq') FAQ
                                li.nav-item
                                    a.nav-link(href='home#') All Services
                                li.nav-item
                                    a.nav-link(href='home#aboutus') About Us
                            .navbar-userlogo
                                a.navbar-userlogo-link(href='#')
                                    img.navbar-userlogo-img(src='../images/userlogo.webp' alt='')
                                a.navbar-userlogo-link(onclick="logOut();" href='/logout')
                                    img.navbar-userlogo-img-exit(src='../images/exit.png' alt='')
section#secuser.secuser
    .container
        .row.secuser-row
            .row
                .secuser-userfoto
                    img.secuser-userfoto-img(src='../images/userfoto.jpg' alt='')
                .col-8.secuser-username
                    span.secuser-username-text
                        span#lastname= lastname
                        span= probel
                        span#name= uname
                        span= probel
                        span#middlename= middlename
                    span.secuser-username-text-subtitle Poet
section#docs.secdoc
    .container
        .row.secdoc-row
            .col-9
                #accordionPanelsStayOpenExample.accordion
                    .accordion-item
                        h2#panelsStayOpen-headingOne.accordion-header
                            button.accordion-button.collapsed.sec4-faq-block(type='button' data-bs-toggle='collapse' data-bs-target='#panelsStayOpen-collapseOne' aria-expanded='true' aria-controls='panelsStayOpen-collapseOne')
                                | Passport
                        #panelsStayOpen-collapseOne.accordion-collapse.collapse(aria-labelledby='panelsStayOpen-headingOne')
                            .accordion-body.row
                                table.col-12.secdoc-passtable
                                    tr.secdoc-passtable-row
                                        td
                                            p.secdoc-passtable-tdtitle Last Name
                                            p#lastname= lastname
                                        td
                                            p.secdoc-passtable-tdtitle First Name
                                            p#firstname= uname
                                        td
                                            p.secdoc-passtable-tdtitle Middle Name
                                            p#middlename= middlename
                                    tr.secdoc-passtable-row
                                        td
                                            p.secdoc-passtable-tdtitle Nationality
                                            p#Nationality= nationality
                                        td
                                            p.secdoc-passtable-tdtitle Document &numero;
                                            p#Documentnum= documentnumber
                                        td
                                            p.secdoc-passtable-tdtitle Authority
                                            p#Authority= dateofexpiry
                                    tr.secdoc-passtable-row
                                        td
                                            p.secdoc-passtable-tdtitle Day Of Birth
                                            p#dayofbirth= dayofbirth
                                        td
                                            p.secdoc-passtable-tdtitle Sex
                                            p#sex= sex
                                        td
                                            p.secdoc-passtable-tdtitle= probel
                                            p#Authority= probel
                                    tr.secdoc-passtable-row
                                        td
                                            p.secdoc-passtable-tdtitle Date Of Issue
                                            p#dateofissue= dateofissue
                                        td
                                            p.secdoc-passtable-tdtitle Date of expiry
                                            p#dateofexpiry= dateofexpiry
                                        td
                                            p.secdoc-passtable-tdtitle= probel
                                            p#Authority= probel
                    .accordion-item
                        h2#panelsStayOpen-headingTwo.accordion-header
                            button.accordion-button.collapsed.sec4-faq-block(type='button' data-bs-toggle='collapse' data-bs-target='#panelsStayOpen-collapseTwo' aria-expanded='false' aria-controls='panelsStayOpen-collapseTwo')
                                | Individual Taxpayer Number
                        #panelsStayOpen-collapseTwo.accordion-collapse.collapse(aria-labelledby='panelsStayOpen-headingTwo')
                            .accordion-body.row
                                .col-12.secdoc-ITN
                                    .secdoc-ITN-title Your number
                                    .secdoc-ITN-number= itn
                    .accordion-item
                        h2#panelsStayOpen-headingTwo.accordion-header
                            button.accordion-button.collapsed.sec4-faq-block(type='button' data-bs-toggle='collapse' data-bs-target='#panelsStayOpen-collapseFour' aria-expanded='false' aria-controls='panelsStayOpen-collapseFour')
                                | Birth Certificate
                        #panelsStayOpen-collapseFour.accordion-collapse.collapse(aria-labelledby='panelsStayOpen-headingFour')
                            .accordion-body.row
                                span#nodata.secdoc-nodata Data not entered
                    .accordion-item
                        h2#panelsStayOpen-headingFive.accordion-header
                            button.accordion-button.collapsed.sec4-faq-block(type='button' data-bs-toggle='collapse' data-bs-target='#panelsStayOpen-collapseFive' aria-expanded='false' aria-controls='panelsStayOpen-collapseFive')
                                | Pension Certificate
                        #panelsStayOpen-collapseFive.accordion-collapse.collapse(aria-labelledby='panelsStayOpen-headingFive')
                            .accordion-body.row
                                span#nodata.secdoc-nodata Data not entered
                    .accordion-item
                        h2#panelsStayOpen-headingSix.accordion-header
                            button.accordion-button.collapsed.sec4-faq-block(type='button' data-bs-toggle='collapse' data-bs-target='#panelsStayOpen-collapseSix' aria-expanded='false' aria-controls='panelsStayOpen-collapseSix')
                                | COVID-Certificate
                        #panelsStayOpen-collapseSix.accordion-collapse.collapse(aria-labelledby='panelsStayOpen-headingSix')
                            .accordion-body.row
                                span#nodata.secdoc-nodata Data not entered
                    .accordion-item
                        h2#panelsStayOpen-headingSeven.accordion-header
                            button.accordion-button.collapsed.sec4-faq-block(type='button' data-bs-toggle='collapse' data-bs-target='#panelsStayOpen-collapseSeven' aria-expanded='false' aria-controls='panelsStayOpen-collapseSeven')
                                | Permit-ID
                        #panelsStayOpen-collapseSeven.accordion-collapse.collapse(aria-labelledby='panelsStayOpen-headingSeven')
                            .accordion-body.row
                                span#nodata.secdoc-nodata Data not entered
footer#footer.footer
    .container
        .row.footer-row
            .col-12
                | Copyright &copy; 2022 -
                span#copydate
                |  by Cloud Goverment Service

script(src='../javascripts/en-US/scripts.js')
